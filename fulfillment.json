{
    "type": "object",
    "$schema": "http://json-schema.org/draft-03/schema",
    "id": "http://jsonschema.net",
    "required": false,
    "properties": {
        "id": {
            "type": "string",
            "id": "http://jsonschema.net/id",
            "required": true,
            "description": "Product Id."
        },
        "shipping": {
            "type": "object",
            "id": "http://jsonschema.net/shipping",
            "required": true,
            "description": "Shipping details.",
            "properties": {
                "dimensions": {
                    "type": "object",
                    "id": "http://jsonschema.net/shipping/dimensions",
                    "required": true,
                    "description": "Dimension of the product.",
                    "properties": {
                        "height": {
                            "type": "number",
                            "id": "http://jsonschema.net/shipping/dimensions/height",
                            "required": true,
                            "description": "Height of the product."
                        },
                        "length": {
                            "type": "number",
                            "id": "http://jsonschema.net/shipping/dimensions/length",
                            "required": true,
                            "description": "Length of the product."
                        },
                        "width": {
                            "type": "number",
                            "id": "http://jsonschema.net/shipping/dimensions/width",
                            "required": true,
                            "description": "Width of the product."
                        }
                    }
                },
                "taxCode": {
                    "type": "number",
                    "id": "http://jsonschema.net/shipping/taxcode",
                    "required": false,
                    "description": "The value of taxcode"
                },
                "minRate": {
                    "type": "number",
                    "id": "http://jsonschema.net/shipping/minRate",
                    "required": false,
                    "description": "Minimum shipping rate. Sparsely populated"
                },
                "soptDays": {
                    "type": "string",
                    "id": "http://jsonschema.net/shipping/soptDays",
                    "required": false,
                    "description": "Standard order processing time. Shipping days is calculated based on this value. Sparsely populated."
                },
                "isSywrMaxShipElig": {
                    "type": "boolean",
                    "id": "http://jsonschema.net/shipping/sywrMaxShipElig",
                    "required": false,
                    "description": "Is item eligible for sywrMaxShipping. Sparsely populated."
                },
                "isIntlShipElig": {
                    "type": "boolean",
                    "id": "http://jsonschema.net/shipping/intlShipElig",
                    "required": false,
                    "description": "Boolean value indicating if the product is eligible for international shipping. Sparsely populated."
                },
                "isMailable": {
                    "type": "boolean",
                    "id": "http://jsonschema.net/shipping/isMailable",
                    "required": true,
                    "description": "Boolean value indicating if the product is mailable."
                },
                "mode": {
                    "type": "object",
                    "id": "http://jsonschema.net/shipping/mode",
                    "required": false,
                    "description": "Applicable shipping modes and price. This data is applicable for marketplace items only. These are the shipping overrides or exceptions specific to a product. If this data is not present then the shipping data of the seller is to be used for all products under the seller.",
                    "properties": {
                        "handlingFee": {
                            "type": "number",
                            "id": "http://jsonschema.net/shipping/mode/mailable/handlingFee",
                            "required": false,
                            "description": "Handling fee."
                        },
                        "exp": {
                            "type": "object",
                            "id": "http://jsonschema.net/shipping/mode/mailable/exp",
                            "required": false,
                            "description": "Express shipping details.",
                            "properties": {
                                "excluded": {
                                    "type": "string",
                                    "id": "http://jsonschema.net/shipping/mode/mailable/exp/excluded",
                                    "required": false,
                                    "description": "Flag governing the exclusion of express shipping for the product."
                                },
                                "price": {
                                    "type": "number",
                                    "id": "http://jsonschema.net/shipping/mode/mailable/exp/price",
                                    "required": false,
                                    "description": "Express shipping price."
                                }
                            }
                        },
                        "gnd": {
                            "type": "object",
                            "id": "http://jsonschema.net/shipping/mode/mailable/gnd",
                            "required": false,
                            "description": "Details of ground shipping.",
                            "properties": {
                                "excluded": {
                                    "type": "string",
                                    "id": "http://jsonschema.net/shipping/mode/mailable/gnd/excluded",
                                    "required": false,
                                    "description": "Flag governing the exclusion of ground shipping for the product."
                                },
                                "free": {
                                    "type": "object",
                                    "id": "http://jsonschema.net/shipping/mode/mailable/gnd/free",
                                    "required": false,
                                    "description": "Start date,End date and promotion text when free ground shipping is applicable.",
                                    "properties": {
                                        "startDt": {
                                            "type": "string",
                                            "id": "http://jsonschema.net/shipping/mode/mailable/gnd/free/startDt",
                                            "required": false,
                                            "description": "ISO Date Format (Example 2013-02-07T01:15:01.090Z). Start Date for free ground shipping"
                                        },
                                        "endDt": {
                                            "type": "string",
                                            "id": "http://jsonschema.net/shipping/mode/mailable/gnd/free/endDt",
                                            "required": false,
                                            "description": "ISO Date Format(Example 2013-02-07T01:15:01.090Z). End Date for free ground shipping"
                                        },
                                        "promoTxt": {
                                            "type": "string",
                                            "id": "http://jsonschema.net/shipping/mode/mailable/gnd/free/promoTxt",
                                            "required": false,
                                            "description": "Text to be displayed while free ground shipping is applicable."
                                        }
                                    }
                                },
                                "price": {
                                    "type": "number",
                                    "id": "http://jsonschema.net/shipping/mode/mailable/gnd/price",
                                    "required": true,
                                    "description": "Price to be applied for Ground shipping when charged."
                                }
                            }
                        },
                        "prem": {
                            "type": "object",
                            "id": "http://jsonschema.net/shipping/mode/mailable/prem",
                            "required": false,
                            "description": "Details of premium shipping.",
                            "properties": {
                                "excluded": {
                                    "type": "string",
                                    "id": "http://jsonschema.net/shipping/mode/mailable/prem/excluded",
                                    "required": false,
                                    "description": "Flag governing the exclusion of premium shipping for the product."
                                },
                                "price": {
                                    "type": "number",
                                    "id": "http://jsonschema.net/shipping/mode/mailable/prem/price",
                                    "required": false,
                                    "description": "Price to be applied for premium shipping."
                                }
                            }
                        },
                        "dmar": {
                            "type": "object",
                            "id": "http://jsonschema.net/shipping/mode/mailable/dmar",
                            "required": false,
                            "description": "Details of Demar shipping.",
                            "properties": {
                                "excluded": {
                                    "type": "string",
                                    "id": "http://jsonschema.net/shipping/mode/mailable/dmar/excluded",
                                    "required": false,
                                    "description": "Flag governing the exclusion of Demar shipping for the product."
                                },
                                "price": {
                                    "type": "number",
                                    "id": "http://jsonschema.net/shipping/mode/mailable/dmar/price",
                                    "required": false,
                                    "description": "Price to be applied for Demar shipping."
                                }
                            }
                        }
                    }
                },
                "freeShip": {
                    "type": "object",
                    "id": "http://jsonschema.net/shipping/freeShip",
                    "required": false,
                    "description": "Free shipping related information.",
                    "properties": {
                        "freeShipThreshold": {
                            "type": "string",
                            "id": "http://jsonschema.net/shipping/freeShip/freeShipThreshold",
                            "required": false,
                            "description": "Minimum purchase value set for free shipping."
                        },
                        "freeShipFlag": {
                            "type": "boolean",
                            "id": "http://jsonschema.net/shipping/freeShip/freeShipFlag",
                            "required": false,
                            "description": "Indicator for free shipping."
                        },
                        "freeDelivery": {
                            "type": "boolean",
                            "id": "http://jsonschema.net/shipping/freeShip/freeDelivery",
                            "required": false,
                            "description": "Indicator for free delivery enabled or not."
                        }
                    }
                },
                "maxQntyPurchasable": {
                    "type": "object",
                    "id": "http://jsonschema.net/shipping/maxQntyPurchasable",
                    "required": false,
                    "description": "Maximum quantity of an item that can be purchased per order.",
                    "properties": {
                        "sites": {
                            "type": "object",
                            "id": "http://jsonschema.net/shipping/maxQntyPurchasable/sites",
                            "required": true,
                            "description": "Site specific info.",
                            "properties": {
                                "sears": {
                                    "type": "object",
                                    "id": "http://jsonschema.net/shipping/maxQntyPurchasable/sites/sears",
                                    "required": false,
                                    "description": "Details of maxQntyPurchasable for sears.",
                                    "properties": {
                                        "val": {
                                            "type": "string",
                                            "id": "http://jsonschema.net/shipping/maxQntyPurchasable/sites/sears/val",
                                            "required": true,
                                            "description": "Maximum quantity of an item specific for sears that can be purchased per order."
                                        }
                                    }
                                },
                                "kmart": {
                                    "type": "object",
                                    "id": "http://jsonschema.net/shipping/maxQntyPurchasable/sites/kmart",
                                    "required": false,
                                    "description": "Details of maxQntyPurchasable for kmart.",
                                    "properties": {
                                        "val": {
                                            "type": "number",
                                            "id": "http://jsonschema.net/shipping/maxQntyPurchasable/sites/kmart/val",
                                            "required": true,
                                            "description": "Maximum quantity of an item specific for kmart that can be purchased per order."
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "weight": {
                    "type": "number",
                    "id": "http://jsonschema.net/shipping/weight",
                    "required": true,
                    "description": "Weight of the product."
                },
                "shipSurcharge": {
                    "type": "number",
                    "id": "http://jsonschema.net/shipping/shipSurcharge",
                    "required": false,
                    "description": "The value of shipping surcharge"
                },
                "isSimiElig": {
                    "type": "boolean",
                    "id": "http://jsonschema.net/shipping/isSimiElig",
                    "required": false,
                    "description": "Eligible or not for third party shipping contract."
                }
            }
        },
        "ffm": {
            "type": "object",
            "id": "http://jsonschema.net/ffm",
            "required": true,
            "description": "Product fulfillment details.",
            "properties": {
                "channel": {
                    "type": "string",
                    "id": "http://jsonschema.net/ffm/channel",
                    "required": true,
                    "description": "Fulfillment Channel."
                },
                "isDeliveryElig": {
                    "type": "boolean",
                    "id": "http://jsonschema.net/ffm/isDeliveryElig",
                    "required": false,
                    "description": "Boolean value determining whether the product is delivery eligible. Sparsely populated."
                },
                "isDoNotEmailMe": {
                    "type": "boolean",
                    "id": "http://jsonschema.net/ffm/isDoNotEmailMe",
                    "required": false,
                    "description": "Boolean value determining whether the product is DontEmailMe. Sparsely populated."
                },
                "isLayawayElig": {
                    "type": "boolean",
                    "id": "http://jsonschema.net/ffm/isLayawayElig",
                    "required": false,
                    "description": "Boolean value determining whether the product is Layaway eligible. Sparsely populated."
                },
                "isShipFromStoreElig": {
                    "type": "boolean",
                    "id": "http://jsonschema.net/ffm/isShipFromStoreElig",
                    "required": false,
                    "description": "Boolean value determining whether the product is shipFromStore eligible. Sparsely populated."
                },
                "isShipElig": {
                    "type": "boolean",
                    "id": "http://jsonschema.net/ffm/isShipElig",
                    "required": true,
                    "description": "Boolean value determining whether the product is  ship eligible."
                },
                "isStoreResElig": {
                    "type": "boolean",
                    "id": "http://jsonschema.net/ffm/isStoreResElig",
                    "required": false,
                    "description": "Boolean value determining whether the product is store reservation eligible. Sparsely populated."
                },
                "isShipToStoreElig": {
                    "type": "boolean",
                    "id": "http://jsonschema.net/ffm/isShipToStoreElig",
                    "required": false,
                    "description": "Boolean value determining whether the product is ship to store eligible. Sparsely populated."
                },
                "shipToStoreChannel": {
                    "type": "string",
                    "id": "http://jsonschema.net/ffm/shipToStoreChannel",
                    "required": false,
                    "description": "The ship to store channel. Possible values for Nonmarketplace:SRES,CRES,DRES,VRES,KDRES and possible values for Marketplace:CVDS"
                },
                "isWebExcl": {
                    "type": "boolean",
                    "id": "http://jsonschema.net/ffm/isWebExcl",
                    "required": false,
                    "description": "Boolean value determining whether the product is web exclusive. Sparsely populated."
                },
                "isSpuElig": {
                    "type": "boolean",
                    "id": "http://jsonschema.net/ffm/isSpuElig",
                    "required": false,
                    "description": "Boolean value determining whether the product is spu eligible."
                },
                "vendorDunsNo": {
                    "type": "string",
                    "id": "http://jsonschema.net/ffm/vendorDunsNo",
                    "required": true,
                    "description": "Vendor duns number."
                },
                "warehouseLocation": {
                    "type": "string",
                    "id": "http://jsonschema.net/ffm/warehouseLocation",
                    "required": true,
                    "description": "Warehouse location"
                },
                "fulfilledBy": {
                    "type": "string",
                    "id": "http://jsonschema.net/ffm/fulfilledBy",
                    "required": false,
                    "description": "For nonmarketplace items it is the parent store value. For marketplace items the following are the values depending on the programType; FBM -> Sellername, FBS -> Sears, DSS -> Sellername"
                },
                "soldBy": {
                    "type": "string",
                    "id": "http://jsonschema.net/ffm/soldBy",
                    "required": false,
                    "description": "For nonmarketplace items it is the parent store value. For marketplace items the following are the values depending on the programType; FBM -> Sellername, FBS -> Sellername, DSS -> Sears"
                },
                "dfltFfmDisplay": {
                    "type": "string",
                    "id": "http://jsonschema.net/ffm/dfltFfmDisplay",
                    "required": false,
                    "description": "Indicates against which fulfillment option will the radio button on the product page will be selected by default. This attribute can have 3 different values --> Delivery, Pickup, Ship"
                }
            }
        },
        "replenishment": {
            "type": "object",
            "id": "http://jsonschema.net/replenishment",
            "required": false,
            "description": "Replenishment details of the product.",
            "properties": {
                "internalRimSts": {
                    "type": "string",
                    "id": "http://jsonschema.net/replenishment/internalRimSts",
                    "required": true,
                    "description": "Internal RIM status for the product. R--> REGULAR,D --> DISCONTINUED,S --> SEASONAL,E --> END OF SEASON,X --> CLEARANCE,T --> PARENT TERMINATED,F --> CMDC DISCONTINUED,C --> CLASS 1,Z --> NOT STORE STOCKED,L --> SOURCE SERVICED,P --> HIGHLY ACTIVE,Q --> AUTO-CREATED"
                },
                "purchaseSts": {
                    "type": "string",
                    "id": "http://jsonschema.net/replenishment/purchaseSts",
                    "required": true,
                    "enum": [
                        "W",
                        "A",
                        "U"
                    ],
                    "description": "Purchase status of the item. Possible values are W -> WHILESUPPLIESLAST, A -> AVAILABLE, U -> UNAVAILABLE."
                },
                "unitCost": {
                    "type": "number",
                    "id": "http://jsonschema.net/replenishment/unitCost",
                    "required": true,
                    "description": "Unit cost of the item."
                },
                "vendorPackId": {
                    "type": "string",
                    "id": "http://jsonschema.net/replenishment/vendorPackId",
                    "required": true,
                    "description": "Vendor pack Id for VD items."
                },
                "canCarryGrp": {
                    "type": "string",
                    "id": "http://jsonschema.net/replenishment/canCarryGrp",
                    "required": false,
                    "description": "The value of cancarry group"
                },
                "cntryOfOrig": {
                    "type": "string",
                    "id": "http://jsonschema.net/replenishment/cntryOfOrig",
                    "required": false,
                    "description": "Country of origin"
                },
                "flowType": {
                    "type": "string",
                    "id": "http://jsonschema.net/replenishment/flowType",
                    "required": false,
                    "description": "Flow type value"
                },
                "coreItemFlag": {
                    "type": "string",
                    "id": "http://jsonschema.net/replenishment/coreItemFlag",
                    "required": false,
                    "description": "Core Item flag also known as product-itemFlag. This data is used to pass to other systems like tranfile "
                },
                "rsosIndicator": {
                    "type": "string",
                    "id": "http://jsonschema.net/replenishment/rsosIndicator",
                    "required": false,
                    "description": "Indicates whether the item is an RSOS (Retail Special Order System) item or not."
                }
            }
        }
    }
}